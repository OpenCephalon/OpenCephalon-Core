{% extends 'OpenCephalonBundle::base.html.twig' %}

{% block title %}Welcome{% endblock %}

{% block main %}



	<ol class="breadcrumb">
		<li class="active">Project {{  project.title }}</li>
		<li class="active">Source {{  source.title }}</li>
		<li class="active">Stream {{  sourceStream.url }}</li>
	</ol>


	<h1>Project {{ project.title }} Source {{ source.title }} Stream {{ sourceStream.url }}</h1>


	<ul class="list-group">
		<li class="list-group-item"><a href="{{  path('opencephalon_project_source_stream_outstreams_list', { 'projectId':project.publicId, 'sourceId': source.publicId, 'streamId':sourceStream.publicId }) }}" class="btn btn-default">Out Streams</a></li>
	</ul>

{%  endblock %}